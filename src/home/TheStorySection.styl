.container
  padding 80px 20px 40px 20px
  background-image: url('./assets/KAAN Show_01 Intro to KAAN (1280x800 px).png');
  background-position: center
  background-size: cover;
  text-align: center
  min-height: calc(100vh - 80px);
  box-sizing: border-box;
  p
